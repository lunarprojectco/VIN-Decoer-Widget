 function startScanner() {
 // Get the VIN input field
 var vinInput = document.getElementById("vin-number");

 // Create a new camera scanner element
 var scanner = document.createElement("iframe");
 scanner.id = "camera-scanner-iframe";
 scanner.src = "https://example.com/scanner.html"; // Replace with the URL of the scanner page

 // Add the scanner element to the page
 var scannerContainer = document.getElementById("camera-scanner");
 scannerContainer.appendChild(scanner);

 // Listen for messages from the scanner
 window.addEventListener("message", function(event) {
 if (event.origin !== "https://example.com") { // Replace with the origin of the scanner page
 return;
 }

 // Get the decoded VIN from the message
 var decodedVin = event.data.vin</script>
